<grammar LANGID="409">
	<RULE NAME="gpsr_actions_rule">
		<L PROPNAME="action">
		<!-- TODO: ADD put / place / release command -->
		<!-- pure ACTIONS --> 
			<P VALSTR="introduce">
				<O><P>...</P></O>
				<L>
					<P>tell something about yourself</P>
					<P>introduce yourself</P>
				</L>
				<O><P>...</P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>

			<P VALSTR="learn_person">
				<O><P>...</P></O>
				<L>
					<P>learn</P>
					<P>memorize</P>
				</L>
				<O><P>...</P></O>
				<P>person</P>
				<O><P> ... </P></O>
				<O> <RULEREF name="locations_rule" url="locations.xml" /> </O>
				<O><P>...</P></O>
			</P>
		<!-- Person finding always follows a (the Person/him/her which differs it from object identification-->	
			<P VALSTR="find_person">
				<O><P>...</P></O>
				<L>
					<P>find</P>
					<P>detect</P>
				</L>
				<O><P>...</P></O>				
				<P>person</P>
				<O><P> ... </P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			<!-- Person identification always follows a (person/him/her which differs it from object identification-->
			<P VALSTR="identify_person">
				<O><P>...</P></O>
				<L>
					<P>identify </P>
					<P>recognize</P>
				</L>
				<O><P>...</P></O>
				<P>person</P>
				<O><P> ... </P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			<P VALSTR="follow_person">
				<O><P>...</P></O>
				<P>follow</P>
				<O><P>...</P></O>
				<P>person</P>
				<O><P> ... </P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			<P VALSTR="guide">
				<O><P>...</P></O>
				<P>guide</P>
				<O><P>...</P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			<!-- ACTIONS with LOCATION--> 
			<P VALSTR="move_to">
				<O><P>...</P></O>			
				<L>
					<P>move</P>
					<P>navigate</P>	
					<P>go</P>
					<P>drive</P>
				</L>
				<O><P> ... </P></O>
				<RULEREF name="locations_rule" url="locations.xml" />
				<O><P>...</P></O>
			</P>
			<!-- ACTIONS with LOCATION--> 
			<P VALSTR="categorize_objects">
				<O><P>...</P></O>			
				<L>
					<P>classify</P>
					<P>categorize</P>	
				</L>
				<O><P> ... </P></O>
				<L>
					<P>object</P>
					<P>objects</P>
				</L>
				<O><P> ... </P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			<!-- ACTIONS with OBJECT--> 
			<P VALSTR="detect_object">
				<O><P>...</P></O>
				<L>
					<P>detect</P>
					<P>find</P>
					<P>identify</P>
				</L>
				<P>the <RULEREF name="objects_rule" url="objects.xml" /> </P>
				<O><P> ... </P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			
			<P VALSTR="point_to_object">
				<O><P>...</P></O>
				<L>
					<P>show</P>
					<P>point to</P>
				</L>
				<P>the <RULEREF name="objects_rule" url="objects.xml" /> </P>
				<O><P> ... </P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			
			<P VALSTR="grasp">
			<!-- ACTIONS with OBJECT and LOCATION--> 	
				<O><P>...</P></O>			
				<L>
					<P>get</P>
					<P>gat</P>
					<P>grasp</P>	
					<P>take</P>
					<P>taik</P>
					<P>teik</P>
					<P>pick</P>
				</L>
				<O><P> ... </P></O>
				<P>the<RULEREF name="objects_rule" url="objects.xml" /> </P>			
				<O><P> ... </P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			<P VALSTR="clean_table">
			<!-- ACTIONS with OBJECT and LOCATION--> 	
				<O><P>...</P></O>			
				<P>clean table</P>
				<O><P> ... </P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			<P VALSTR="weight">
				<O><P>...</P></O>			
				<L>
					<P>weight bottle</P>
					<P>weight object</P>
					<P>weight it</P>
				</L>
			</P>
			<P VALSTR="release">
				<O><P>...</P></O>			
				<L>
					<P>release</P>
				</L>
				<O><P> ... </P></O>
				<L>
					<P>object</P>
					<P>bottle</P>
					<P>it</P>
				</L>
				<O><P> ... </P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			<P VALSTR="handover">
				<O><P>...</P></O>			
				<L>
					<P>hand</P>
					<P>deliver</P>
					<P>handover</P>
					<P>hand over</P>
				</L>
				<O><P> ... </P></O>
				<L>
					<P>object</P>
					<P>bottle</P>
					<P>it</P>
				</L>
				<O><P>over</P></O>
				<O><P> ... </P></O>
				<O><RULEREF name="locations_rule" url="locations.xml" /></O>
				<O><P>...</P></O>
			</P>
			<P VALSTR="carry_box">
				<O><P>...</P></O>			
				<L>
					<P>carry</P>
					<P>carry box</P>
					<P>lift</P>
					<P>pick up</P>
				</L>
				<O><P> ... </P></O>
				<L>
					<P>box</P>
					<P>beer box</P>
				</L>
				<O><P>...</P></O>
			</P>
		<!--	<P VALSTR="exit">
				<O><P>...</P></O>			
				<L>
					<P>exit</P>
					<P>leave</P>
					<P>move to charge</P>
					<P>shutdown</P>
				</L>
				<O><P>...</P></O>
			</P>-->
		</L>
	</RULE>
</grammar>
